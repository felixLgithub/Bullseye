import turtle
t = turtle.Turtle()
t.color("red")
t.begin_fill()
t.circle(100)
t.end_fill()
t.color("black")
t.begin_fill()
t.circle(80)
t.end_fill()
t.color("red")
t.begin_fill()
t.circle(60)
t.end_fill()
t.color("black")
t.begin_fill()
t.circle(40)
t.end_fill()
t.color("red")
t.begin_fill()
t.circle(20)
t.end_fill()
