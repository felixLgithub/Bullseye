import turtle
t = turtle.Turtle()

t.color("red")
t.begin_fill()
t.circle(100)
t.end_fill()

t.left(90)
t.forward(20)
t.right(90)

t.color("black")
t.begin_fill()
t.circle(80)
t.end_fill()

t.left(90)
t.forward(20)
t.right(90)

t.color("red")
t.begin_fill()
t.circle(60)
t.end_fill()

t.left(90)
t.forward(20)
t.right(90)

t.color("black")
t.begin_fill()
t.circle(40)
t.end_fill()

t.left(90)
t.forward(20)
t.right(90)

t.color("red")
t.begin_fill()
t.circle(20)
t.end_fill()
